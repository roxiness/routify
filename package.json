    "cheap-watch": "^1.0.2",
    "commander": "^5.0.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.3",
    "log-symbols": "^3.0.0",
    "picomatch": "^2.2.1",
    "readdirp": "^3.3.0",
    "rollup-pluginutils": "^2.8.2",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@roxi/routify": "*",
    "chalk": "^4.0.0",
    "chokidar-cli": "^2.1.0",
    "del-cli": "^3.0.0",
    "memfs": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "rewiremock": "^3.13.9",
    "semantic-release": "^17.1.1",
