{
  "name": "routify-test-example",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roxi/routify.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "run-p routify rollup mock-server",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "mock-server": "nodemon api-server",
    "build": "npm run routify -- -b && rollup -c",
    "serve": "spassr",
    "export": "routify export",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.9.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-workbox": "^5.0.1",
    "spassr": "^2.2.3",
    "svelte": "^3.20.1",
    "workbox-build": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "nodemon": "^2.0.7"
  },
  "routify": {
    "plugins": {
      "routify-dev-ui": {}
    }
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "routify-dev-ui": "latest",
    "workbox-window": "^5.1.3"
  },
  "spassr": {
    "entrypoint": "dist/__app.html",
    "script": "dist/build/bundle.js"
  },
  "workspaces": [
    "../.."
  ]
}
